body, h1, h2, h3, h4, h5, h6 {
    font-family: 'Venti', Calibri, Futura, sans-serif;
}


@font-face {
    font-family: 'Venti';
    src: url('venticf-demibold-webfont.eot');
    src: url('venticf-demibold-webfont.eot?#iefix') format('embedded-opentype'),
         url('venticf-demibold-webfont.woff2') format('woff2'),
         url('venticf-demibold-webfont.woff') format('woff'),
         url('venticf-demibold-webfont.ttf') format('truetype'),
         url('venticf-demibold-webfont.svg#venti_cfdemi_bold') format('svg');
    font-weight: bold;
    font-style: normal;
}

@font-face {
    font-family: 'Venti';
    src: url('venticf-demiboldoblique-webfont.eot');
    src: url('venticf-demiboldoblique-webfont.eot?#iefix') format('embedded-opentype'),
         url('venticf-demiboldoblique-webfont.woff2') format('woff2'),
         url('venticf-demiboldoblique-webfont.woff') format('woff'),
         url('venticf-demiboldoblique-webfont.ttf') format('truetype'),
         url('venticf-demiboldoblique-webfont.svg#venti_cfdemi_bold_oblique') format('svg');
    font-weight: bold;
    font-style: oblique;
}

